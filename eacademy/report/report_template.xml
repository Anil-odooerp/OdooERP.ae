<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_course">
        <t t-call="web.basic_layout">
            <main>
                <div class="page">
                    <div class="oe_structure"/>

                    <head>
                        <title>Course Report</title>
                        <!--    CSS -->
                        <style>
                            body {
                            margin: 0;
                            font-family: Arial, sans-serif;
                            }

                            .container {
                            display: flex;
                            }

                            .left-column {
                            flex: 1;
                            padding: 20px;
                            }

                            .center-column {
                            flex: 2;
                            padding: 20px;
                            }

                            .right-column {
                            flex: 1;
                            padding: 20px;
                            }

                            footer {
                            padding: 20px;
                            text-align: left;

                            }

                            .table {
                            width: 100%;
                            border-collapse: collapse;
                            }

                            .table th,.table td {
                            padding: 8px;
                            text-align: left;
                            border-bottom: 1px dashed #007bff;
                            }

                            .dashed-row {
                            border-bottom: 1px dashed #007bff;
                            }

                        </style>
                    </head>
                    
                    <body>
                        <div class="container">

                            <!--        Left corner  -->

                            <div class="left-column">
                                <img src="data:image/jpeg;base64,"
                                     alt="Your Image" style="width:100px;"/>
                                <h4>Eacademy</h4>
                                <p>Date:<t t-esc="time.strftime('%Y-%m-%d')"/><br/>Noida, Delhi<br/>India<br/>TEL: 9829159839
                                </p>
                            </div>

                            <!--        Center -->

                            <div class="center-column">
                                <h3>My Company (Odoo)</h3>
                                <p>United Arab Emirates<br/>U.A.E<br/>TEL: 9829159839
                                </p>
                            </div>


                            <!--        Right corner -->

                            <div class="right-column">
                                <div style="text-align: right;">
                                    <h3>COURSE LIST</h3>
                                    <t t-esc="course_list"/>
                                </div>
                                <div style="border-top: 1px solid #000000; padding: 5px;"></div>
                                <table class="table table-sm o_main_table">
                                    <!--               <thead>   -->
                                    <tr>
                                        <th>Name</th>
                                        <th>Age</th>
                                        <th>Course</th>
                                        <th>Start Date</th>
                                    </tr>

                                    <!--             </thead>   -->
                                    <tbody>

                                        <tr t-foreach="docs" t-as="doc">
                                            <td>
                                                <span t-field="doc.name"/>
                                            </td>
                                            <td>
                                                <span t-field="doc.age"/>
                                            </td>
                                            <td>
                                                <span t-field="doc.course"/>
                                            </td>
                                            <td>
                                                <span t-field="doc.start_date"/>
                                            </td>
                                        </tr>
                                        <tr>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                        <!--    Center -->

                        <div class="center-column">
                            <div style="border-top: 1px solid #007bff; padding: 5px;">
                                <div class="date-amount-container"
                                     style="display: flex; justify-content: space-between;">
                                    <div>Date:
                                        <t t-esc="time.strftime('%Y-%m-%d')"/>
                                    </div>
                                    <div>Amount:
                                        <t t-esc="amount"/>
                                    </div>
                                </div>
                                <div style="border-top: 1px solid #ccc; margin: 5px 0;"></div>
                                <div style="text-align: right;">Total Amount:
                                    <t t-esc="total_amount"/>
                                </div>
                                <div style="border-top: 1px solid #ccc; margin: 5px 0;"></div>
                            </div>
                        </div>
                        <p>
                            <div style="border-top: 1px solid #ccc; margin: 5px 0;"></div>
                        </p>

                        <!--  Footer  -->

                        <footer>
                            <p>This report provides a detailed overview of the courses available in the eAcademy system.</p>
                            <p>Terms and conditions :</p>
                            <p>Validity :</p>
                            <p>Payment Terms :</p>
                            <p>Address:</p>
                        </footer>
                    </body>

                </div>
            </main>
        </t>
    </template>

</odoo>








